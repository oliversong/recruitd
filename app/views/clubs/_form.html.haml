- form_for @club do |f|
  = f.error_messages
  %p
    = f.label :term_id
    %br
    = f.text_field :term_id
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.label :added_by_user_id
    %br
    = f.text_field :added_by_user_id
  %p
    = f.submit
