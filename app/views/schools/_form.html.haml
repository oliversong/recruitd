- form_for @school do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :term_id
    %br
    = f.text_field :term_id
  %p
    = f.label :address_line1
    %br
    = f.text_field :address_line1
  %p
    = f.label :address_line2
    %br
    = f.text_field :address_line2
  %p
    = f.label :city
    %br
    = f.text_field :city
  %p
    = f.label :state
    %br
    = f.text_field :state
  %p
    = f.label :zip
    %br
    = f.text_field :zip
  %p
    = f.submit
