<h1>Listing school_students</h1>

<table>
  <tr>
    <th>School</th>
    <th>Student</th>
    <th>Startdate</th>
    <th>Enddate</th>
    <th>Details</th>
  </tr>

<% @school_students.each do |school_student| %>
  <tr>
    <td><%=h school_student.school_id %></td>
    <td><%=h school_student.student_id %></td>
    <td><%=h school_student.startdate %></td>
    <td><%=h school_student.enddate %></td>
    <td><%=h school_student.details %></td>
    <td><%= link_to 'Show', school_student %></td>
    <td><%= link_to 'Edit', edit_school_student_path(school_student) %></td>
    <td><%= link_to 'Destroy', school_student, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New school_student', new_school_student_path %>