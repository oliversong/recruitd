%p
	%b Labels:
	%ul
		-@company.labels.each do |lbl|
			%li
				= link_to lbl.name, "\##{lbl.id}"
				
-@company.labels.each do |lbl|
	%a{:name => "#{lbl.id}"}
	%p
		%b= lbl.name
		%ul
			- lbl.company_labelings.each do |labeling|
				= link_to labeling.company.name, labeling.company
				%br
				Labels:
				- labeling.student.company_labelings.where(:company_id => @company.id).each do |other_lbl|
					= other_lbl.label.name
					|
				- sf = @company_files.find{|item| item.student_id == labeling.student_id }
				- if sf
					%br/
					rating: #{sf.rating}
					-if sf.starred
						| starred